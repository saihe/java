public class List {
	String[][] list = new String[6][];
	operation op = new operation();

	//データをを配列
	public String[][] listUp(){
		for(int i = 0; i < op.date.size() ; i++){
			list[0][i] = op.date.get(i);
			list[1][i] = op.time.get(i);
			list[2][i] = op.what.get(i);
			list[3][i] = op.plus.get(i);
			list[4][i] = op.minus.get(i);
			list[5][i] = op.result.get(i);
		}
		return list;
	}


}
