public class Data {
	String[][] data = new String[6][];
	operation op = new operation();

	//データをを配列
	public String[][] listUp(){
		for(int i = 0; i < op.date.size() ; i++){
			data[0][i] = op.date.get(i);
			data[1][i] = op.time.get(i);
			data[2][i] = op.what.get(i);
			data[3][i] = op.plus.get(i);
			data[4][i] = op.minus.get(i);
			data[5][i] = op.result.get(i);
		}
		return data;
	}


}
