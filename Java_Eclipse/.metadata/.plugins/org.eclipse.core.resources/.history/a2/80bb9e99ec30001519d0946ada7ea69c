public class Data {
	String[][] data = new String[6][];
	//ArrayList<ArrayList<String>> list = new ArrayList<ArrayList <String>>();
	//ArrayList<String> data = new ArrayList<String>();
	//data.add(op.time.get(i));
	//list.add(data);

	operation op = new operation();

	//日付を配列
	public String[][] dateUp(){
		for(int i = 0; i < op.date.size() ; i++){
			data[0][i] = op.date.get(i);
		}
		return data;
	}

	//時間を配列
	public String[][] timeUp(){
		for(int i = 0; i < op.date.size() ; i++){
			data[1][i] = op.time.get(i);
		}
		return data;
	}

	//科目を配列
	public String[][] whatUp(){
		for(int i = 0; i < op.date.size() ; i++){
			data[2][i] = op.what.get(i);
		}
		return data;
	}

	//入金を配列
	public String[][] plusUp(){
		for(int i = 0; i < op.date.size() ; i++){
			data[2][i] = op.plus.get(i);
		}
		return data;
	}

	//出金を配列
	public String[][] minusUp(){
		for(int i = 0; i < op.date.size() ; i++){
			data[2][i] = op.minus.get(i);
		}
		return data;
	}


}
