import java.util.ArrayList;

public class Data {
	ArrayList<ArrayList<String>> list = new ArrayList<ArrayList <String>>();
	ArrayList<String> data = new ArrayList<String>();

	/*
	data.add(要素);
	list.add(data);//二次元目に格納！
	*/
	operation op = new operation();

	//日付を配列
	public ArrayList<ArrayList<String>> dateUp(){
		for(int i = 0; i < op.date.size() ; i++){
			data.add(op.date.get(i));
			list.add(data);
		}
		return list;
	}

	//時間を配列
	public ArrayList<ArrayList<String>> timeUp(){
		for(int i = 0; i < op.date.size() ; i++){
			data.add(op.time.get(i));
			list.add(data);
		}
		return list;
	}

	//科目を配列
	public ArrayList<ArrayList<String>> whatUp(){
		for(int i = 0; i < op.date.size() ; i++){
			data.add(op.time.get(i));
			list.add(data);
		}
		return list;
	}


}
